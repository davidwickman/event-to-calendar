{
  "manifest_version": 2,
  "name": "Event to Calendar",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/events/*"],
      "js": ["js/moment.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "calendar-icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
